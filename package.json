    "url": "git+https://github.com/zachkrall/artloop.git"
  },
  "keywords": [
    "cli",
    "art",
    "display",
    "slideshow"
  ],
  "author": "Zach Krall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zachkrall/artloop/issues"
  },
  "homepage": "https://github.com/zachkrall/artloop#readme",
  "dependencies": {
    "binary-install": "0.0.1"
  },
  "files": [
    "npm/**/*"
  ]
}